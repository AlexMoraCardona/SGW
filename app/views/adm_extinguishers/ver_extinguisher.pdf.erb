        <% @extinguishers.each do |extinguishers| %>
           <tr>
               <td style="border:0px solid black;">
                   <% if extinguishers.extinguisher_fotos.attached? %>
                       <hr>
                       <div class="form-group">
                           <h5>Archivos adjuntos extintor Nro: <%= extinguishers.nro %></h5>
                       </div>         
                       <div data-controller="previews" >
                           <% extinguishers.extinguisher_fotos.each do |file|%>
                                <br>
                                <% if file.image? %>
                                    <div>
                                        <%= file.blob.filename %>
                                        <%= image_tag file, data: {previews_target: "preview"}, class: "image" %>
                                    </div> 
                                <% elsif file.video? %>
                                    <%= file.blob.filename %>
                                    <%= video_tag url_for(file), controls: "", width: "400px" %>          
                                <% elsif file.audio? %>
                                    <%= file.blob.filename %>
                                    <%= audio_tag(url_for(file), controls: "") %>          
                                <% else %>
                                    <%= link_to file.filename, url_for(file) %>          
                                <% end %>
                            <% end %>  
                        </div>
                    <% end %>  
