<header>
    <table style="width: 100%;">
        <tr>
            <td rowspan="2"; style="width: 25%; text-align:center; border:1px solid black">
                <% if @kit.entity.logo.present? then %>
                  <%=  image_tag  @kit.entity.logo, class: "image_peq" %>
                <% end %>   
            </td>
            <td style="width: 75%; border:1px solid black; text-align:center">
                <div><h6><%= @kit.entity.business_name if @kit.present? %></h6></div>
            </td>
        </tr>
        <tr>   
            <td style="width: 75%; border:1px solid black; text-align:center">
                <div><h6><%= @template.name if @template.present? %></h6></div>
            </td>
        </tr>
    </table>
    <table style="width: 100%;">
        <tr>    
            <td style="border:1px solid black; width: 25%">
                <h6>CÓDIGO: </h6>                
            </td>
            <td style="border:1px solid black; width: 25%">
                <h6>&nbsp;&nbsp;<small class="text-muted"><%= @template.reference if @template.present? %></small></h6>                
