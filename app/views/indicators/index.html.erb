<div align="left"; class="container col-12">
    <a class="navbar-brand" href="/">
        <%= image_tag 'LogoConesst202305232.png',:width => "100",  :height=>"100" %>
    </a>
</div>
<div class="container col-11">
        <div class="d-flex">
            <div class="container">
               <h2> <%= t('.title')%></h2>
            </div> 
        </div>
        <div>
            <%= link_to t('.create_indicator'), new_indicator_path, class: 'btn btn-sm btn-primary' if Current.user.level > 0 && Current.user.level < 3 %>
        </div>
        <hr>
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col"><%= t('.name') %></th>
                <th scope="col"><%= t('.description') %></th>
                <th scope="col"><%= t('common.show') %></th>
                <th scope="col"><%= t('common.edit') if Current.user.level > 0 && Current.user.level < 3  %></th>
            </tr>
        </thead>
        <tbody>
            <%= render partial: 'indicator', collection: @indicators  %>  
        </tbody>      
    </table>
</div>
